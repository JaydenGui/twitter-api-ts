{
  "name": "twitter-api-ts",
  "main": "./target/index.js",
  "typings": "./target/index.d.ts",
  "version": "0.0.7",
  "scripts": {
    "lint": "tslint --project tsconfig.json --type-check",
    "compile": "tsc",
    "cleanTarget": "rm -rf ./target/*",
    "prepublishOnly": "npm run cleanTarget && npm run compile && npm run lint",
    "format": "prettier --write --tab-width 4 --print-width 100 --single-quote --trailing-comma all '!(target)/**/*.{ts,js}'"
  },
  "files": [
    "target"
  ],
  "dependencies": {
    "decode-ts": "0.0.2",
    "fp-ts": "0.3.1",
    "io-ts": "0.4.0",
    "node-fetch": "1.7.1",
    "oauth-authorization-header": "0.0.5"
  },
  "devDependencies": {
    "@types/node-fetch": "1.6.7",
    "prettier": "1.4.4",
    "tslint": "5.4.3",
    "tslint-language-service": "0.9.6",
    "typescript": "next",
    "typescript-string-enums": "0.3.4"
  }
}
